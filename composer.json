{
    "name": "z-golly/elastic",
    "description": "elasticsearch for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "jonathan",
            "email": "jianyang1209@outlook.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/annotations": "^1.12",
        "elasticsearch/elasticsearch": "^7.11",
        "illuminate/collections": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/events": "^8.0",
        "illuminate/pagination": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Golly\\Elastic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Golly\\Elastic\\ElasticServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
